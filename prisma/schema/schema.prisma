generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["cari", "public"]
}

model categorias {
  idcategoria Int     @id @default(autoincrement())
  categoria   String? @db.VarChar(255)
  descripcion String? @db.VarChar(1000)

  @@map("categorias")
  @@schema("cari")
}

model compras {
  idcompra    Int       @id @default(autoincrement())
  fecha       DateTime? @db.Date
  idproducto  Int?
  cantidad    Int?
  importe     Float?
  idproveedor Int?
  moneda      String?   @db.VarChar(50)
  nro_factura String?   @db.VarChar
  observacion String?   @db.VarChar(500)

  @@schema("cari")
}

model productos {
  idproducto      Int       @id @default(autoincrement())
  producto        String?   @db.VarChar(255)
  alta            DateTime? @db.Date
  unidad          String?   @db.VarChar(10)
  stock           Int?
  observacion     String?   @db.VarChar(1000)
  precio_unitario Float?
  estado          Boolean?
  idcategoria     Int?
  moneda          String?   @db.VarChar(10)

  @@map("productos")
  @@schema("cari")
}

model usuarios {
  usuario    String  @db.VarChar(255)
  contrasena String  @db.VarChar(255)
  apellido   String  @db.VarChar(255)
  nombre     String  @db.VarChar(255)
  estado     Boolean
  idusuario  Int     @id @default(autoincrement())

  @@schema("cari")
}

model vehiculos {
  idvehiculo  Int      @id @default(autoincrement())
  vehiculo    String?  @db.VarChar(255)
  patente     String?  @db.VarChar(255)
  modelo      Int?
  observacion String?  @db.VarChar(1000)
  estado      Boolean?

  @@schema("cari")
}

model Account {
  userId            String
  type              String
  provider          String
  providerAccountId String
  refresh_token     String?
  access_token      String?
  expires_at        Int?
  token_type        String?
  scope             String?
  id_token          String?
  session_state     String?
  createdAt         DateTime @default(now())
  updatedAt         DateTime
  User              User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@id([provider, providerAccountId])
  @@schema("public")
}

model User {
  id            String    @id
  name          String?
  email         String    @unique
  password      String?
  emailVerified DateTime?
  image         String?
  role          Role      @default(user)
  createdAt     DateTime  @default(now())
  updatedAt     DateTime
  tenantName    String?   @db.VarChar(255)
  tenantID      Int?
  estado        Boolean?
  Account       Account[]
  Tenant        Tenant?   @relation(fields: [tenantID], references: [tenantID])

  @@schema("public")
}

model VerificationToken {
  identifier String   @id @unique
  token      String
  expires    DateTime

  @@schema("public")
}

model proveedores {
  idproveedor      Int      @id @default(autoincrement())
  proveedor        String?  @db.VarChar(255)
  clave_tributaria String?  @db.VarChar(20)
  tipo_clave       String?  @db.VarChar(20)
  domicilio        String?  @db.VarChar(500)
  observacion      String?  @db.VarChar(500)
  estado           Boolean?
  telefono         String?  @db.VarChar(50)

  @@schema("cari")
}

model ventas {
  idventa     Int       @id(map: "ventas_copy1_pkey") @default(autoincrement())
  fecha       DateTime? @db.Date
  idproducto  Int?
  cantidad    Int?
  importe     Float?
  idcliente   Int?
  moneda      String?   @db.VarChar(50)
  nro_factura String?   @db.VarChar
  observacion String?   @db.VarChar(500)

  @@schema("cari")
}

model clientes {
  idcliente Int      @id(map: "clientes_copy1_pkey") @default(autoincrement())
  cliente   String?  @db.VarChar(255)
  dni       String?  @db.VarChar(20)
  calle     String?  @db.VarChar(255)
  numero    Int?
  barrio    String?  @db.VarChar(255)
  telefono  String?  @db.VarChar(50)
  mail      String?  @db.VarChar(255)
  estado    Boolean?
  nombre    String?  @db.VarChar(255)

  @@schema("cari")
}

model empleados {
  idempleado    Int       @id @default(autoincrement())
  cuil          String?   @db.VarChar(20)
  dni           String?   @db.VarChar(20)
  apellido      String?   @db.VarChar(255)
  nombre        String?   @db.VarChar(255)
  fecha_ingreso DateTime? @db.Date
  estado        Boolean?  @default(true)
  idcategoria   Int?

  @@map("empleados")
  @@schema("cari")
}

model facturacion {
  idfactura   Int      @id @default(autoincrement())
  fecha       DateTime @db.Date
  idproveedor Int?
  nro_factura String?  @db.VarChar(255)
  descripcion String?  @db.VarChar(1000)
  importe     Float?
  moneda      String?  @db.VarChar(10)
  anio        Int?

  @@schema("cari")
}

model haberes {
  idhaber               Int       @id @default(autoincrement())
  idempleado            Int?
  fecha_pago            DateTime? @db.Date
  dias_trabajo          Int?
  cant_unitario         Float?
  haberes               Float?
  antiguedad_porcentaje Float?
  antiguedad_monto      Float?
  asig_familiar         Float?
  no_remunerado         Float?
  premio                Float?
  haberes_brutos        Float?
  jubilacion            Float?
  ley_19032             Float?
  obra_social           Float?
  renatea               Float?
  cta_solidaria         Float?
  total_deducciones     Float?
  haberes_neto          Float?
  anticipo              Float?
  cancelacion           Float?

  @@schema("cari")
}

model Tenant {
  tenantID     Int      @id(map: "empresa_pkey")
  tenantName   String?  @db.VarChar(255)
  razon_social String?  @db.VarChar(255)
  cuit_cuil    String?  @db.VarChar(255)
  direccion    String?  @db.VarChar(255)
  telefono     String?  @db.VarChar(50)
  email        String?  @db.VarChar(255)
  ubicacion    String?  @db.VarChar(255)
  estado       Boolean?
  empresa      String?  @db.VarChar(255)
  User         User[]

  @@schema("public")
}

model empelados_categorias {
  idcategoria Int     @id @default(autoincrement())
  categoria   String? @db.VarChar(255)
  descripcion String? @db.VarChar(1000)

  @@schema("cari")
}

enum Role {
  user
  admin

  @@schema("public")
}
